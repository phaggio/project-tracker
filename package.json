{
  "name": "project-tracker",
  "version": "1.0.0",
  "description": "project-tracker",
  "main": "server/dist/server.js",
  "engines": {
    "node": ">=14.3.0"
  },
  "scripts": {
    "heroku-postbuild": "npm run start",
    "build": "cd client && npm run build",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "seed": "node server/dist/seeds/seedDB.js",
    "start": "node server/dist/server.js",
    "start:dev": "concurrently \"npm run tsc-server\" \"nodemon \" \"npm run client\"",
    "start:dev-server": "concurrently \"npm run tsc-server\" \"nodemon \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc-server": "cd server/src && tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaggio/project-tracker.git"
  },
  "author": "phaggio",
