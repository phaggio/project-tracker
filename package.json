    "start": "node dist/server.js",
    "start:dev": "concurrently \"tsc -w\" \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc"
  },
