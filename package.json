    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.26"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.2"
