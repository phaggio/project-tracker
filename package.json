{
  "name": "project-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "cd client && npm run build",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "seed": "concurrently \"tsc\" \"node dist/seeds/seedDB.js\"",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "tsc && node dist/server.js",
    "start:dev": "concurrently \"tsc -w\" \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaggio/project-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phaggio/project-tracker/issues"
  },
  "homepage": "https://github.com/phaggio/project-tracker#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.26"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
